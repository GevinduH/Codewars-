https://www.codewars.com/kata/sum-of-two-lowest-positive-integers

function sumTwoSmallestNumbers(numbers) {     
  numbers.sort(function (a, b) {    
    return a - b;   
  });    
  return numbers[0] + numbers[1];   
};   


https://www.codewars.com/kata/remove-the-minimum/javascript    

function removeSmallest(numbers) {   
  while (numbers) {    
    const newNumbers = numbers.slice()   
    let smallest = newNumbers[0]   
    for (let i=0;i<numbers.length;i++) {   
     if (newNumbers[i]<=smallest) {    
       smallest =numbers[i];     
     } continue;    
    }    
    const index = newNumbers.indexOf(smallest)   
    newNumbers.splice(index,1)    
    return newNumbers;    
  } return numbers;    
    
}     

https://www.codewars.com/kata/the-highest-profit-wins    
    
function minMax(arr){     
  const min =  Math.min(...arr);     
  const max = Math.max(...arr);    
  return [min, max];     
}     

