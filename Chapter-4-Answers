function filter_list(l) {
   return newList = l.filter((item)=> { return typeof item === 'number'})
}

https://www.codewars.com/kata/5502c9e7b3216ec63c0001aa

function openOrSenior(data){
	return data.map((element)=>{
		if (element[0] >= 55 && element[1] > 7) {
			return 'Senior';
		}return 'Open';		
	})
}

https://www.codewars.com/kata/return-the-day/javascript

function whatday(num) { 
	var obj = {
		1:"Sunday",
		2:"Monday",
		3:"Tuesday",
		4:"Wednesday",
		5:"Thursday",
		6:"Friday",
		7:"Saturday"
	}
  if (num <= 7 && num>=1) {
	return obj[num];
  }return "Wrong, please enter a number between 1 and 7";

}

https://www.codewars.com/kata/beginner-lost-without-a-map/javascript

function maps(x){
	let doubledX;
	return doubledX = x.map((num)=>num*2);
}

https://www.codewars.com/kata/array-plus-array/javascript

function arrayPlusArray(arr1, arr2) {
	const array = arr1.concat(arr2);
	return array.reduce((a, b) => a + b);
}

